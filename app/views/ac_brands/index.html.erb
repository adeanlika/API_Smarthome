<p id="notice"><%= notice %></p>

<h1>Ac Brands</h1>

<table>
  <thead>
    <tr>
      <th>Brand</th>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ac_brands.each do |ac_brand| %>
      <tr>
        <td><%= ac_brand.brand %></td>
        <td><%= ac_brand.code %></td>
        <td><%= link_to 'Show', ac_brand %></td>
        <td><%= link_to 'Edit', edit_ac_brand_path(ac_brand) %></td>
        <td><%= link_to 'Destroy', ac_brand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ac Brand', new_ac_brand_path %>
